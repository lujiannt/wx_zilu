      </view>
    </view>
  </view> -->

  <!-- <wxc-input type="text" mode="normal" placeholder-style="font-size:17px" placeholder="请输入收货人姓名"></wxc-input> -->

  <view class="page__bd page__bd_spacing">
    <view class="timeView">
      <canvas class="progress_bg" canvas-id="canvasProgressbg"></canvas>
      <canvas class="progress_canvas" canvas-id="canvasProgress"> </canvas>
      <view class="progress_text">
        <view class="progress_dot"></view>
        <text class='progress_info' wx:if="{{countTime != null}}">{{countTime}}</text>

        <wxc-input  wx:if="{{clickFlag != true}}" type="text" mode="normal" style="font-size:16px;text-align: center" placeholder-style="font-size:16px;text-align: center" placeholder="本次专注的目标" bind:blur="onBlur" value="{{aim}}"/>
      </view>
    </view>
    <view class="button-sp-area">
      <button class="btn1" hover-class='hover_btn1' bindtap='startFanQie'>开始           </button>
    </view>
  </view>
</view>