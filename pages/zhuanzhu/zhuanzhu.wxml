<view class="page">
  <view class="page__hd">
    <view class="page__title"></view>
    <view class="page__desc textCenter">{{defaultAim}}</view>
  </view>

  <!-- <view class="weui-cells weui-cells_after-title myinput">
    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
          <view class="weui-label">qq</view>
